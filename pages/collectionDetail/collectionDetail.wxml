<view class="top" style="{{cData.outStyle}}{{!!navHeight?'padding-top:'+navHeight+'px':''}}">
    <view class="content">
        <view class="title_box">
            <view class="title">{{cData.title}}</view>
            <view class="desc">{{cData.subTitle}}</view>
        </view>
        <view class="tool_box">
            <view class="collect_box" catchtap="likeOrUnlike">
                <image src="/images/collected.png" class="collect" wx:if="{{cData.isLike}}"></image>
                <image src="/images/collect.png" class="collect" wx:else></image>
            </view>
            <view class="info_box" catchtap="showInfo">
                <image src="/images/i.png" class="i_icon"></image>
            </view>
        </view>
    </view>
</view>
<view class="container">
    <view class="cell left" wx:for="{{cData.list}}" style="{{'background:#'+item.html}};height:93.75rpx;width:93.75rpx"></view>
</view>
<navBar navType="circle" navBgStyle="{{navBgStyle}}" bindGoBack="goBack"></navBar>