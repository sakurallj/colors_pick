<view class='colors_container'>
    <view class='color_item' wx:for="{{colors.builtInArrs}}" style='background:#{{item.color3}}' bindtap='chooseItem' data-item="{{item}}">
        <view class='color_item_cell_2' style='background:#{{item.color1}}'></view>
        <view class='color_item_cell_1' style='background:#{{item.color0}}'></view>
        <view class='color_item_cell_3' style='background:#{{item.color2}}'></view>
        <view class='color_item_text' >{{index+1}}</view>
    </view>
</view>
<dialog id="dialog" bind:cancelEvent="cancelEvent" dialog_type="{{dialog_type}}" d_data="{{dialogData}}"></dialog>