<view class="container">
    <view class="text_back" style="{{!!navHeight?'height:'+navHeight+'px':''}}" wx:if="{{navStyle=='text'}}">
        <view class="text_back_box" style="{{!!boxMTop?'margin-top: '+boxMTop+'px;':''}} {{!!boxHeight?'height: '+boxHeight+'px;':''}}">
            <image class="text_back_image" bindtap="goBack" style="{{!!imageWidth?'width: '+imageWidth+'px;':''}}{{!!imageHeight?'height: '+imageHeight+'px;':''}}" src="/images/angle-left-red.png"></image>
            <text class="text_back_text" bindtap="goBack" style="{{!!textFontSize?'font-size: '+textFontSize+'px;':''}}">返回</text>
        </view>
    </view>
    <view class="circle_back" style="{{!!navHeight?'height:'+navHeight+'px':''}}" wx:if="{{navStyle=='circle'}}">
        <view class="circle_box {{!!isShowHome?'home':''}}" style="{{!!boxMTop?'margin-top: '+boxMTop+'px;':''}}   ">
            <block wx:if="{{isShowHome}}">
                <view class="circle_image_left_box" bindtap="goBack">
                    <image class="circle_image_left" src="/images/left_white.png"></image>
                </view>
                <image class="circle_image_line" src="/images/line.png"></image>
                <view class="circle_image_home_box" bindtap="backHome">
                    <image class="circle_image_home" src="/images/home.png"></image>
                </view>
            </block>
            <block wx:else>
                <image class="circle_image" bindtap="goBack" src="/images/angle-left-circle-black.png"></image>
            </block>
        </view>
    </view>

    <view class="browse" style="{{!!navHeight?'height:'+navHeight+'px':''}}" wx:if="{{navStyle=='browse'}}">
        <view class="browse_box" style="{{!!boxMTop?'margin-top: '+boxMTop+'px;':''}} ">
            <text class="browse_text" bindtap="goBack">{{navTitle}}</text>
        </view>
    </view>


    <view class="n_t" style="{{!!navHeight?'height:'+navHeight+'px':''}}" wx:if="{{navStyle=='title'}}">
        <view class="n_title_box" style="{{!!boxMTop?'margin-top: '+boxMTop+'px;':''}} ">
            <view class="n_title">{{navTitle}}</view>
        </view>
    </view>

</view>