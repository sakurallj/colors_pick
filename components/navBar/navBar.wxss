.container {
}

.text_back, .browse, .n_t {
    position: fixed;
    top: 0;
    left: 0;
    width: 750rpx;
    background: #fffefb;
}

.text_back_box {
    display: flex;
    height: 32px;
    align-items: center;
    margin-left: 40rpx;
}

.text_back_image {
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
}

.text_back_text {
    color: #ff6661;
    font-weight: 800;
    margin-left: 10rpx;
    font-size: 40rpx;
}

.circle_back {
    position: fixed;
    top: 0;
    left: 0;
    width: 750rpx;
}

.circle_box {
    height: 32px;
    border-radius: 32px;
    margin-left: 40rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 32px;
}

.circle_image_left_box, .circle_image_home_box {
    height: 30px;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 39px;
}

.circle_image_left {
    width: 16px;
    height: 16px;
    display: block;
}

.circle_image_home {
    width: 16px;
    height: 16px;
    display: block;
}

.circle_image_line {
    width: 5px;
    height: 19px;
}

.circle_box.home {
    height: 30px;
    border-radius: 32px;
    margin-left: 40rpx;
    display: flex;
    align-items: center;
    background: rgba(0, 0, 0, 0.15);
    border: 1px solid rgba(255, 255, 255, 0.2);
    justify-content: space-between;
    width: 78px;
}

.circle_image {
    width: 100%;
    height: 100%;
    opacity: 0.2;
}

.browse_box {
    display: flex;
    height: 32px;
    align-items: flex-start;
    margin-left: 40rpx;
    width: 100px;
}

.browse_text {
    font-size: 30px;
    line-height: 32px;
    color: #474b4a;
}

.n_title_box {
    display: flex;
    height: 32px;
    align-items: center;
    width: 750rpx;
    justify-content: center;
}

.n_title {
    font-size: 18px;
    line-height: 32px;
    font-weight: 600;
    color: #474b4a;
}
